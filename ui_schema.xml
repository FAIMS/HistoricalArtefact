<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa">
  <!-- ****** -->
  <!--FAIMS UI Schema generated by Pierre Estephan, Wednesday 2nd April-->
  <!-- ****** -->

  <h:head>
    <h:title>HistoricalArtefacts</h:title>
    <!--model lists user interface tabgroups, tabs and content of tabs-->
    <model>
      <instance>
        <faims id="HistoricalArtefacts">
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
          </style>
          <user>
            <usertab>
              <users/>
            </usertab>
          </user>
          <control>
            <Artefact>
              <newGlass/>
              <Glass/>
              <loadGlass/>
              <newMetal/>
              <Metal/>
              <loadMetal/>
              <newCeramic/>
              <Ceramic/> 
              <loadCeramic/>
            </Artefact>
          </control>

          <Glass>
            <Admin>
              <SurveyPoint/>
              <FieldID/>
              <CatalogueNumber/>
              <Easting/>
              <Northing/>
              <Elevation/>
              <Update/>
            </Admin>
            <Glass>
              <Diagnostic/>
              <Colour/>
              <Completeness/>
              <FragLocation/>
              <VesselType/>
              <TablewareType/>
              <FragLength/>
              <FragWeight/>
              <FragWidth/>
              <FragThickness/>
              <GroupedFrag/> <!-- yes/no -->
              <takePhoto/>
              <Update/>
            </Glass>
            <GroupedFrags> <!-- originally hidden -->
              <NumberFrags/>
              <GroupWeight/>
              <GroupMaxLength/>
              <GroupMaxWidth/>
              <GroupMaxThickness/>
              <GroupMinLength/>
              <GroupMinWidth/>
              <GroupMinThickness/>
              <Update/>
            </GroupedFrags>
            <BaseSherds>
              <BaseDiameter/>
              <BaseThickness/>
              <KickUpDepth/>
              <PontilMark/>
              <TechnologicalBaseMarks/>
              <Update/>
            </BaseSherds>
            <Closure>
              <BoreDiameter/>
              <FinishType/>
              <ClosureType/>
              <AppliedFinish/>
              <StretchMarks/>
              <Update/>
            </Closure>
            <BodySherds>
              <Horizontal/>
              <ProbableContents/>
              <ShoulderWidth/>
              <CompleteHeight/>
              <NumberOfMouldSeams/>
              <MouldType/>
            </BodySherds>
            <OtherInfo>
              <TrademarksLocations/>
              <DateRange/>
              <Description/>
              <Reference/>
              <Update/>
            </OtherInfo>
          </Glass>

          <Metal>
            <Admin>
              <SurveyPoint/>
              <FieldID/>
              <CatalogueNumber/>
              <Easting/>
              <Northing/>
              <Elevation/>
              <Update/>
            </Admin>
            <Metal>
              <MetalType/>
              <Completeness/>
              <Length/>
              <Thickness/>
              <Width/>
              <Weight/>
              <Function/>
              <PortionComponent/>
              <Modification/>
              <Buttons/>
              <NailsFasteners/>
              <takePhoto/>
              <Update/>
            </Metal>
            <Buttons> <!-- hidden -->
              <AttachmentMethod/>
              <LigneSize/>
              <ManufactureMethod/>
              <Construction/>
              <ShankType/>
              <SewThroughType/>
              <NumberOfEyes/>
              <Update/>
            </Buttons>
            <NailsFasteners> <!-- hidden -->
              <ManufactureMethod/>
              <Form/>
              <SizeClass/>
              <HeadShape/>
              <ShaftCrossSection/>
              <ShaftShape/>
              <Features/>
              <Update/>
            </NailsFasteners>
            <OtherInfo>
              <MotifTrademarks/>
              <DateRange/>
              <Description/>
              <Reference/>
              <Update/>
            </OtherInfo>
          </Metal>

          <Ceramic>
            <Admin>
              <SurveyPoint/>
              <FieldID/>
              <CatalogueNumber/>
              <Easting/>
              <Northing/>
              <Elevation/>
              <Update/>
            </Admin>
            <Ceramic>
              <Diagnostic/>
              <Waretype/>
              <Type/>
              <Element/>
              <Form/>
              <Completeness/>
              <FunctionalityType/>
              <GroupedFrag/>
              <Length/>
              <Thickness/>
              <Width/>
              <Weight/>
              <BaseDiameter/>
              <RimDiameter/>
              <tooSmall/>
              <ArcLength/>
              <tooSmallTwo/>
              <takePhoto/>
              <Update/>
            </Ceramic>
            <Decorations>
              <PasteColour/>
              <GlazeType/>
              <DecorationMethod/>
              <DecorationColours/>
              <VisibleMotifs/>
              <DecorationPatternName>
              <Update/>
            </Decorations>
            <OtherInfo>
              <MakersMarkTrademarks/>
              <DateRange/>
              <Description/>
              <Reference/>
              <Update/>
            </OtherInfo>
          </Ceramic>
        </faims>
      </instance>
      <bind nodeset="/faims/Glass/Glass/FragLength" type="decimal"/>
      <bind nodeset="/faims/Glass/Glass/FragWeight" type="decimal"/>
      <bind nodeset="/faims/Glass/Glass/FragWidth" type="decimal"/>
      <bind nodeset="/faims/Glass/Glass/FragThickness" type="decimal"/>
      <bind nodeset="/faims/Glass/GroupedFrags/NumberFrags" type="decimal"/>
      <bind nodeset="/faims/Glass/GroupedFrags/GroupWeight" type="decimal"/>
      <bind nodeset="/faims/Glass/GroupedFrags/GroupMaxLength" type="decimal"/>
      <bind nodeset="/faims/Glass/GroupedFrags/GroupMaxWidth" type="decimal"/>
      <bind nodeset="/faims/Glass/GroupedFrags/GroupMaxThickness" type="decimal"/>
      <bind nodeset="/faims/Glass/GroupedFrags/GroupMinLength" type="decimal"/>
      <bind nodeset="/faims/Glass/GroupedFrags/GroupMinWidth" type="decimal"/>
      <bind nodeset="/faims/Glass/GroupedFrags/GroupMinThickness" type="decimal"/>
      <bind nodeset="/faims/Glass/BaseSherds/BaseDiameter" type="decimal"/>
      <bind nodeset="/faims/Glass/BaseSherds/BaseThickness" type="decimal"/>
      <bind nodeset="/faims/Glass/BaseSherds/KickUpDepth" type="decimal"/>
      <bind nodeset="/faims/Glass/Closure/BoreDiameter" type="decimal"/>
      <bind nodeset="/faims/Glass/BodySherds/ShoulderWidth" type="decimal"/>
      <bind nodeset="/faims/Glass/BodySherds/CompleteHeight" type="decimal"/>
      <bind nodeset="/faims/Glass/BodySherds/NumberOfMouldSeams" type="decimal"/>
      <bind nodeset="/faims/Metal/Metal/Length" type="decimal"/>
      <bind nodeset="/faims/Metal/Metal/Thickness" type="decimal"/>
      <bind nodeset="/faims/Metal/Metal/Width" type="decimal"/>
      <bind nodeset="/faims/Metal/Metal/Weight" type="decimal"/>
      <bind nodeset="/faims/Ceramic/Ceramic/Length" type="decimal"/>
      <bind nodeset="/faims/Ceramic/Ceramic/Thickness" type="decimal"/>
      <bind nodeset="/faims/Ceramic/Ceramic/Width" type="decimal"/>
      <bind nodeset="/faims/Ceramic/Ceramic/Weight" type="decimal"/>
      <bind nodeset="/faims/Ceramic/Ceramic/BaseDiameter" type="decimal"/>
      <bind nodeset="/faims/Ceramic/Ceramic/RimDiameter" type="decimal"/>
      <bind nodeset="/faims/Ceramic/Ceramic/ArcLength" type="decimal"/>
    </model>
  </h:head>
  <h:body>
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
    </group>
    <group ref="user">
      <label>{User_List}</label>
      <group ref="usertab" faims_scrollable="false">
        <label>{User_List}</label>
        <select1 appearance="compact" ref="users">
          <label>{Users}:</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
      </group>
    </group>
    <group ref="control">
      <label>Artefact</label>
      <!-- Artefacts -->
      <group ref="Artefact">
        <label>Artefacts</label>
        <trigger ref="newGlass">
          <label>Add Glass</label>
        </trigger>
        <select1 ref="Glass" faims_attribute_name="Glass" faims_certainty="false" faims_attribute_type="vocab" faims_read_only="false">
          <label>Glass</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <trigger ref="loadGlass">
          <label>Load Glass</label>
        </trigger>
        <trigger ref="newMetal">
          <label>Add Metal</label>
        </trigger>
        <select1 ref="Metal" faims_attribute_name="Metal" faims_certainty="false" faims_attribute_type="vocab" faims_read_only="false">
          <label>Metal</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <trigger ref="loadMetal">
          <label>Load Metal</label>
        </trigger>
        <trigger ref="newCeramic">
          <label>Add Ceramic</label>
        </trigger>
        <select1 ref="Ceramic" faims_attribute_name="Ceramic" faims_certainty="false" faims_attribute_type="vocab" faims_read_only="false">
          <label>Ceramic</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <trigger ref="loadCeramic">
          <label>Load Ceramic</label>
        </trigger>
      </group>
    </group>


    <!-- Glass -->
    <group ref="Glass" faims_archent_type="Glass">
      <label>Glass</label>
      <!-- Admin Tab -->
      <group ref="Admin">
        <label>Admin</label>
        <select1 ref="SurveyPoint" faims_attribute_name="Survey Point" faims_certainty="false" appearance="full" faims_attribute_type="vocab" faims_read_only="false">
          <label>Survey Point</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <input ref="FieldID" faims_attribute_name="Field ID" faims_certainty="true" faims_attribute_type="freetext" faims_read_only="false">
          <label>Field ID</label>
        </input>
        <input ref="CatalogueNumber" faims_attribute_name="Catalogue Number" faims_certainty="true" faims_attribute_type="freetext" faims_read_only="true">
          <label>Catalogue Number</label>
        </input>
        <input ref="Easting" faims_attribute_name="Easting" faims_certainty="true" faims_attribute_type="freetext" faims_read_only="true">
          <label>Easting</label>
        </input>
        <input ref="Northing" faims_attribute_name="Northing" faims_certainty="true" faims_attribute_type="freetext" faims_read_only="true">
          <label>Northing</label>
        </input>
        <input ref="Elevation" faims_attribute_name="Elevation" faims_certainty="true" faims_attribute_type="freetext" faims_read_only="true">
          <label>Elevation</label>
        </input>
        <trigger ref="Update">
          <label>Save</label>
        </trigger>
      </group>
      <!-- Glass tab -->
      <group ref="Glass">
        <label>Glass</label>
        <input ref="GlassID" faims_attribute_name="Glass ID" faims_certainty="false" faims_attribute_type="freetext">
          <label>Glass ID</label>
        </input>
        <select1 ref="Diagnostic" faims_attribute_name="Diagnostic" appearance="full" faims_certainty="false" faims_attribute_type="vocab" faims_read_only="false">
          <label>Diagnostic</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="Colour" faims_attribute_name="Colour" faims_certainty="false" faims_attribute_type="vocab" faims_read_only="false">
          <label>Colour</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="Completeness" faims_attribute_name="Completeness" faims_certainty="false" faims_attribute_type="vocab" faims_read_only="false">
          <label>Completeness</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="FragLocation" faims_attribute_name="Frag Location" faims_certainty="false" faims_attribute_type="vocab" faims_read_only="false">
          <label>Frag Location</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="VesselType" faims_attribute_name="Vessel Type" faims_certainty="false" faims_attribute_type="vocab" faims_read_only="false">
          <label>VesselType</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="TablewareType" faims_attribute_name="Tableware Type" faims_certainty="false" faims_attribute_type="vocab" faims_read_only="false">
          <label>Tableware Type</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <input ref="FragLength" faims_attribute_name="Frag Length" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Frag Length (mm)</label>
        </input>
        <input ref="FragWeight" faims_attribute_name="Frag Weight" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Frag Weight (g)</label>
        </input>
        <input ref="FragWidth" faims_attribute_name="Frag Width" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Frag Width (mm)</label>
        </input>
        <input ref="FragThickness" faims_attribute_name="Frag Thickness" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Frag Thickness (mm)</label>
        </input>
        <select1 ref="GroupedFrag" faims_attribute_name="Grouped Frag" faims_certainty="true" appearance="full" faims_attribute_type="vocab" faims_read_only="false">
          <label>Grouped Frag</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <trigger ref="takePhoto">
          <label>Take Picture</label>
        </trigger>
        <trigger ref="Update">
          <label>Save Record</label>
        </trigger>
      </group>
      <!-- Grouped Frags tab (originally hidden) -->
      <group ref="GroupedFrags" hidden="true">
        <label>Grouped Frags</label>
        <input ref="NumberFrags" faims_attribute_name="Number of Frags" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Number of Frags</label>
        </input>
        <input ref="GroupWeight" faims_attribute_name="Group Weight" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Group Weight (g)</label>
        </input>
        <input ref="GroupMaxLength" faims_attribute_name="Group Max Length" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Group Max Length (mm)</label>
        </input>
        <input ref="GroupMaxWidth" faims_attribute_name="Group Max Width" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Group Max Width (mm)</label>
        </input>
        <input ref="GroupMaxThickness" faims_attribute_name="Group Max Thickness" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Group Max Thickness (mm)</label>
        </input>
        <input ref="GroupMinLength" faims_attribute_name="Group Min Length" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Group Min Length (mm)</label>
        </input>
        <input ref="GroupMinWidth" faims_attribute_name="Group Min Width" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Group Min Width (mm)</label>
        </input>
        <input ref="GroupMinThickness" faims_attribute_name="Group Min Thickness" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Group Min Thickness (mm)</label>
        </input>
        <trigger ref="Update">
          <label>Save Record</label>
        </trigger>
      </group>
      <!-- Bases and base sherds tab -->
      <group ref="BaseSherds">
        <label>Bases and Base Sherds</label>
        <input ref="BaseDiameter" faims_attribute_name="Base Diameter" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Base Diameter</label>
        </input>
        <input ref="BaseThickness" faims_attribute_name="Base Thickness" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Base Thickness</label>
        </input>
        <input ref="KickUpDepth" faims_attribute_name="Kick Up Depth" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Kick Up Depth</label>
        </input>
        <select1 ref="PontilMark" faims_attribute_name="PontilMark" faims_certainty="true" appearance="full" faims_attribute_type="vocab" faims_read_only="false">
          <label>Pontil Mark</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="TechnologicalBaseMarks" faims_attribute_name="TechnologicalBaseMarks" faims_certainty="false" faims_attribute_type="vocab" faims_read_only="false">
          <label>Technological Base Marks</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <trigger ref="Update">
          <label>Save Record</label>
        </trigger>
      </group>
      <!-- Closure/Finish Sherds Tab -->
      <group ref="Closure">
        <label>Closure</label>
        <input ref="BoreDiameter" faims_attribute_name="BoreDiameter" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Bore Diameter (mm)</label>
        </input>
        <select1 ref="FinishType" faims_attribute_name="FinishType" faims_certainty="false" faims_attribute_type="vocab" faims_read_only="false">
          <label>Finish Type</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="ClosureType" faims_attribute_name="ClosureType" faims_certainty="false" faims_attribute_type="vocab" faims_read_only="false">
          <label>Closure Type</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="AppliedFinish" faims_attribute_name="AppliedFinish" faims_certainty="true" appearance="full" faims_attribute_type="vocab" faims_read_only="false">
          <label>Applied Finish?</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="StretchMarks" faims_attribute_name="StretchMarks" faims_certainty="true" appearance="full" faims_attribute_type="vocab" faims_read_only="false">
          <label>Stretch marks on rock?</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <trigger ref="Update">
          <label>Save Record</label>
        </trigger>
      </group>
      <!-- Body sherds tab -->
      <group ref="BodySherds">
        <label>Body Sherds</label>
        <select1 ref="Horizontal" faims_attribute_name="Horizontal" faims_certainty="false" faims_attribute_type="vocab" faims_read_only="false">
          <label>Horizontal</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="ProbableContents" faims_attribute_name="ProbableContents" faims_certainty="false" faims_attribute_type="vocab" faims_read_only="false">
          <label>Probable Contents</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <input ref="ShoulderWidth" faims_attribute_name="ShoulderWidth" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Shoulder Width (mm)</label>
        </input>
        <input ref="CompleteHeight" faims_attribute_name="CompleteHeight" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Complete Height (mm)</label>
        </input>
        <input ref="NumberOfMouldSeams" faims_attribute_name="NumberOfMouldSeams" faims_certainty="true" faims_attribute_type="measure" faims_read_only="false">
          <label>Number of Mould Seams</label>
        </input>
        <select1 ref="MouldType" faims_attribute_name="MouldType" faims_certainty="false" faims_attribute_type="vocab" faims_read_only="false">
          <label>Mould Type</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <trigger ref="Update">
          <label>Save Record</label>
        </trigger>
      </group>
      <!-- Other info tab -->
      <group ref="OtherInfo">
        <label>Other Information</label>
        <input ref="TrademarksLocations" faims_attribute_name="TrademarksLocations" faims_certainty="true" faims_attribute_type="freetext" faims_read_only="true">
          <label>Trademarks and Locations</label>
        </input>
        <input ref="DateRange" faims_attribute_name="DateRange" faims_certainty="true" faims_attribute_type="freetext" faims_read_only="true">
          <label>Date Range</label>
        </input>
        <input ref="Description" faims_attribute_name="Description" faims_certainty="true" faims_attribute_type="freetext" faims_read_only="true">
          <label>General Description</label>
        </input>
        <input ref="Reference" faims_attribute_name="Reference" faims_certainty="true" faims_attribute_type="freetext" faims_read_only="true">
          <label>Reference</label>
        </input>
        <trigger ref="Update">
          <label>Save Record</label>
        </trigger>
      </group>
    </group>

    <!-- Metal -->

    <group ref="Metal" faims_archent_type="Metal">
      <label>Metal</label>
      <!-- Admin Tab -->
      <group ref="Admin">
        <label>Admin</label>
        <select1 ref="SurveyPoint" faims_attribute_name="Survey Point" faims_certainty="false" appearance="full" faims_attribute_type="vocab" faims_read_only="false">
          <label>Survey Point</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <input ref="FieldID" faims_attribute_name="Field ID" faims_certainty="true" faims_attribute_type="freetext" faims_read_only="false">
          <label>Field ID</label>
        </input>
        <input ref="CatalogueNumber" faims_attribute_name="Catalogue Number" faims_certainty="true" faims_attribute_type="freetext" faims_read_only="true">
          <label>Catalogue Number</label>
        </input>
        <input ref="Easting" faims_attribute_name="Easting" faims_certainty="true" faims_attribute_type="freetext" faims_read_only="true">
